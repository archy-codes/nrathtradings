<div class="contact-background">
	<img alt="" src="${images}/contact/contactCover.png"
		class="contactCover" />
</div>
<div class="contact-form-wrapper"> 
	<div class="form-header">
		<h1>GET IN TOUCH</h1>
		<p>LEAVE US A MESSAGE AND WE WILL GET BACK TO YOU.</p>
	</div>
	<div class="contact-form">
		<form action="" class="main-form">
			<div class="form-top">
				<div class="form-group">
					<label>FIRST NAME</label> <input type="text"
						placeholder="Please enter your name..." class="form-control" />
				</div>
				<div class="form-group">
					<label>LAST NAME</label> <input type="text"
						placeholder="Please enter last name..." class="form-control" />
				</div>
				<div class="form-group">
					<label>EMAIL</label> <input type="text"
						placeholder="Please enter your email..." class="form-control" />
				</div>
				<div class="form-group">
					<label>PHONE NUMBER</label> <input type="text"
						placeholder="Please enter phone number..." class="form-control" />
				</div>
			</div>
			<div class="form-group">
				<label>LEAVE US A MESSAGE</label>
				<textarea rows="5" cols="5" class="form-control" placeholder="enter your message"></textarea>
			</div>
			<div class="form-group">
				<button class="btn btn-primary">SUBMIT</button>
			</div>
		</form>
	</div>
</div>